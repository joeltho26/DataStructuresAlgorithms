    "def nth_fibonacci(n):\n",
    "    if n <= 1:\n",
    "        return n\n",
    "    else:\n",
    "        return nth_fibonacci(n-1) + nth_fibonacci(n-2)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    n = 10\n",
    "    result = nth_fibonacci(n)\n",
    "    print(result)\n",
    "        "
