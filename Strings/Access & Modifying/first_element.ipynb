   "source": [
    "def access_first(string,character_count=None):\n",
    "    if not character_count:\n",
    "        return string[0]\n",
    "    \n",
    "    if character_count:\n",
    "        return string[:character_count]\n",
    "    \n",
    "a = \"abcbaabcaababcad\"\n",
    "print(access_first(a))\n",
    "character_count = 3\n",
    "print(access_first(a,character_count))"
   ]
  }
